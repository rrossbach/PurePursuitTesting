namespace org.sch.vulcanrobotics.fbs;

/// Pose2d is a translation and a rotation
///    - translation is aribitrary units;  rotation is in radians
table Pose2d_fb {
   x:double;
   y:double;
   cosTheta:double;
   sinTheta:double;
}

table Pose2dWithCurvature_fb {
   pose:Pose2d_fb;
   curvature:double;
}

